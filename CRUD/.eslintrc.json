{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "func-names":"off",
      "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "eqeqeq": ["error", "always"],
    "no-unused-vars": ["warn"],
    "no-undef": ["warn"],

    
   
    }  
  }